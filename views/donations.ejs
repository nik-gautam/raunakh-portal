<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Donations</title>
</head>
<body>

    <!-- table heading -->
    <% donators.forEach(function(donator){ %>
        <!-- table row -->
        <%= donator.email %> <br>
        <%= donator.contact %> <br>
        <%= donator.method %> <br>
        <%= donator.amount %> <br>
        <%= donator.currency %> <br>
        <%= donator.order_id %> <br>
        <%= donator.date_created %>
        <!-- table row end -->
    <% }); %>
    <!-- table end -->
<a href="/logout">logout</a>
</body>
</html>
