<!-- newsletter-area-start -->
<div class="newsletter-02-area pt-70 pb-70 bg-warning" id="contact-container">
    
    <div class="container">
        <div class="row">
            <div class="col-xl-8 col-lg-8 offset-lg-2 offset-xl-2">
                 <div class="section-title text-center mb-80">
                     
                     <h1>Contact Us</h1>
                 </div>
            </div>
        </div>
        <div class="newsletter-bg">
            <div class="row">
                <div class="col-xl-6 col-lg-6">
                    <div class="newsletters-wrapper mb-30">
                        <div class="newsletter-form newsletter-02-form">
                            <form action="#">
                                <input id="contact-name" placeholder="Name" type="text">
                            </form>
                        </div>
                    </div>
                </div>
                <div class="col-xl-6 col-lg-6">
                    <div class="newsletters-wrapper mb-30">
                        <div class="newsletter-form newsletter-02-form">
                            <form action="#">
                                <input id="contact-email" placeholder="Email" type="email">
                            </form>
                        </div>
                    </div>
                </div>
                <div class="col-xl-6 col-lg-6">
                    <div class="newsletters-wrapper mb-30">
                        <div class="newsletter-form newsletter-02-form">
                            <form action="#">
                                <input id="contact-subject" placeholder="Subject" type="text">
                            </form>
                        </div>
                    </div>
                </div>
                <div class="col-xl-6 col-lg-6">
                    <div class="newsletters-wrapper mb-30">
                        <div class="newsletter-form newsletter-02-form">
                            <form action="#">
                                <input id="contact-message" placeholder="Message" type="text">
                            </form>
                        </div>
                    </div>
                </div>
                <div class="col-xl-3 col-lg-3">
                    <div class="newsletters-wrapper mb-30">
                        <div class="news-button">
                            <button class="btn" onclick="return validationContact()" type="submit" id="button-contact">Connect now <i class="far fa-long-arrow-right"></i></button>
                        </div>
                    </div>
                </div>
                <div class="col-xl-9 col-lg-9" id="error-contact" >
                   
                </div>
            </div>
        </div>
    </div>
</div>
<!-- newsletter-area-start -->