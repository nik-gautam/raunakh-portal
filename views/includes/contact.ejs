<!-- newsletter-area-start -->
<div class="newsletter-02-area pt-70 pb-70 bg-warning">
    <div class="container">
        <div class="newsletter-bg">
            <div class="row">
                <div class="col-xl-12 col-lg-12">
                    <div class="newsletters-wrapper mb-30">
                        <div class="newsletter-form newsletter-02-form">
                            <form action="#">
                                <input id="contact-name" placeholder="Enter your name" type="text" onblur="contactInput(event)">
                            </form>
                        </div>
                    </div>
                </div>
                <div class="col-xl-12 col-lg-12">
                    <div class="newsletters-wrapper mb-30">
                        <div class="newsletter-form newsletter-02-form">
                            <form action="#">
                                <input id="contact-email" placeholder="Enter your email" type="email">
                            </form>
                        </div>
                    </div>
                </div>
                <div class="col-xl-12 col-lg-12">
                    <div class="newsletters-wrapper mb-30">
                        <div class="newsletter-form newsletter-02-form">
                            <form action="#">
                                <input id="contact-subject" placeholder="Subject" type="text">
                            </form>
                        </div>
                    </div>
                </div>
                <div class="col-xl-12 col-lg-12">
                    <div class="newsletters-wrapper mb-30">
                        <div class="newsletter-form newsletter-02-form">
                            <form action="#">
                                <input id="contact-message" placeholder="Message" type="text">
                            </form>
                        </div>
                    </div>
                </div>
                <div class="col-xl-12 col-lg-12" id="error-contact" >
                   
                </div>
                <div class="col-xl-3 col-lg-3">
                    <div class="newsletters-wrapper mb-30">
                        <div class="news-button">
                            <button class="btn" onclick="valiationContact()" type="submit" id="button-contact">Subscribe now <i class="far fa-long-arrow-right"></i></button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- newsletter-area-start -->