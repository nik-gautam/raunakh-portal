<!-- newsletter-area-start -->
<div class="newsletter-02-area pt-70 pb-70" id="contact-container" style="background-color: #d34848;">
    
    <div class="container">
        <div class="row">
            <div class="col-xl-8 col-lg-8 offset-lg-2 offset-xl-2">
                 <div class="section-title text-center mb-80">
                     
                     <h1 style="color: #f3ee65;">Contact Us</h1>
                 </div>
            </div>
        </div>
        <div class="newsletter-bg">
            <div class="row">
                <div class="col-12">
                    <div class="newsletters-wrapper mb-30">
                        <div class="newsletter-form newsletter-02-form">
                            <form action="/" method="POST">
                                <div class="row">
                                    <div class="col-xl-6 col-lg-6 col-12 mb-3">
                                        <input id="contact-name" placeholder="Name" type="text" name="contactname">
                                    </div>
                                    <div class="col-xl-6 col-lg-6 col-12 mb-3">
                                        <input id="contact-email" placeholder="Email" type="email" name="contactemail">
                                    </div>
                                    <div class="col-xl-6 col-lg-6 col-12 mb-3">
                                        <input id="contact-subject" placeholder="Subject" type="text" name="contactsubject">
                                    </div>
                                    <div class="col-xl-6 col-lg-6 col-12 mb-3">
                                        <input id="contact-message" placeholder="Message" type="text" name="contactmessage">
                                    </div>
                                </div>
                                <button class="btn" type="submit" onclick="return validationContact()" type="submit" id="button-contact">Connect now <i class="far fa-long-arrow-right"></i></button>
                            </form>
                        </div>
                    </div>
                </div>
                
                
    
                
            </div>
        </div>
    </div>
</div>
<!-- newsletter-area-start -->